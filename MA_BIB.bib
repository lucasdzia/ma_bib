@article{gonzalezCaracterizacionSuelosSeis2021,
  title = {{Caracterizaci{\'o}n de los suelos de seis municipios en Norte de Santander}},
  author = {Gonzalez, Leonides Castellanos and {Gonz{\'a}lez-Pedraza}, Ana Francisca and Mogoll{\'o}n, Alfonso Eugenio Capacho},
  year = {2021},
  journal = {Inge CuC},
  volume = {17},
  number = {1},
  pages = {69--80},
  issn = {2382-4700},
  doi = {10.17981/ingecuc.17.1.2021.06},
  urldate = {2024-11-01},
  abstract = {Introduction- The main axis of economic development in the North Department of Santander is the agricultural sector. However, there is no productive management plan that allows the planning and efficient promotion of land use. Objective- To determine the taxonomic, agrochemical and physical characteristics of the soils in six municipalities of\&nbsp; Norte de Santander department. Methodology- The study was carried out on 90 farms in Arboledas, Convention, Esperanza, La Playa, Mutiscua and Oca{\~n}a municipalities. Samples of soils composed of 0 to 30 cm (2 ha/farm) were taken and texture and macro and micronutrients were determined. A descriptive statistical analysis was performed for all soil variables analyzed. The method of samples not paired by the Student t test was used with a probability of error of 5\%. The statistical package SPSS version 21 was used. Results- Taxonomically the soils belong to the orders Inceptisol and Entisol. The predominant textural classes were loam, silty clay loam and sandy loam. Macro and micronutrients concentration was low with the exception of iron and manganese and the coefficients of variation were above 50\% for most of the elements analyzed. Conclusions- The predominated soils were Inceptisols with Dystrudepts, Eutrudepts Humudepts and Dystrustepts suborders, and Entisol with Udorthents and Ustorthents suborders, with sandy loam, silty loam,, loam and silty clay loam textures. These presented a high variability in the concentrations of macro and micronutrients and in general low natural fertility.\&nbsp;},
  copyright = {Derechos de autor 2020 INGE CUC},
  langid = {spanish},
  keywords = {fertilidad natural,niveles nutricionales,propiedades fisico quimicas,taxonomia de suelo},
  file = {C:\Users\lucas\Zotero\storage\HTFIJRQP\Gonzalez et al. - 2021 - Caracterización de los suelos de seis municipios e.pdf}
}

